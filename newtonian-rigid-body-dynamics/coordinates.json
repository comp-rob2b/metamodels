{
    "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "dyn-coord": "https://comp-rob2b.github.io/metamodels/newtonian-rigid-body-dynamics/coordinates#",

        "MassReference": "dyn-coord:MassReference",
        "MassScalar": "dyn-coord:MassScalar",
        "InertiaReference": "dyn-coord:InertiaReference",
