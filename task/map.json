{
    "@context": {
        "@version": 1.1,
        "map": "https://comp-rob2b.github.io/metamodels/task/map#",

        "View": {
            "@id": "map:View",
            "@context": {
                "superobject": {
                    "@id": "map:superobject",
                    "@type": "@id"
                },
                "subobject": {
                    "@id": "map:subobject",
                    "@type": "@id"
                }
            }
        },

        "PoseCoordinateView": {
            "@id": "map:PoseCoordinateView",
            "@context": {
                "subspace": {
                    "@id": "map:subspace",
                    "@type": "@vocab",
                    "@context": {
                        "position": "map:position"
                    }
                },
                "axis": {
                    "@id": "map:axis",
                    "@type": "@vocab",
                    "@context": {
                        "x": "map:x",
                        "y": "map:y",
                        "z": "map:z"
                    }
                }
            }
        },
        "VelocityTwistCoordinateView": {
            "@id": "map:VelocityTwistCoordinateView",
            "@context": {
                "subspace": {
                    "@id": "map:subspace",
                    "@type": "@vocab",
                    "@context": {
                        "angular-velocity": "map:angular-velocity",
                        "linear-velocity": "map:linear-velocity"
                    }
                },
                "axis": {
                    "@id": "map:axis",
                    "@type": "@vocab",
                    "@context": {
                        "x": "map:x",
                        "y": "map:y",
                        "z": "map:z"
                    }
                }
            }
        },
        "AccelerationTwistCoordinateView": {
            "@id": "map:AccelerationTwistCoordinateView",
            "@context": {
                "subspace": {
                    "@id": "map:subspace",
                    "@type": "@vocab",
                    "@context": {
                        "angular-acceleration": "map:angular-acceleration",
                        "linear-acceleration": "map:linear-acceleration"
                    }
                },
                "axis": {
                    "@id": "map:axis",
                    "@type": "@vocab",
                    "@context": {
                        "x": "map:x",
                        "y": "map:y",
                        "z": "map:z"
                    }
                }
            }
        },
        "WrenchCoordinateView": {
            "@id": "map:WrenchCoordinateView",
            "@context": {
                "subspace": {
                    "@id": "map:subspace",
                    "@type": "@vocab",
                    "@context": {
                        "torque": "map:torque",
                        "force": "map:force"
                    }
                },
                "axis": {
                    "@id": "map:axis",
                    "@type": "@vocab",
                    "@context": {
                        "x": "map:x",
                        "y": "map:y",
                        "z": "map:z"
                    }
                }
            }
        }
    }
}
