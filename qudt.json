{
    "@context": {
        "@version": 1.1,
        "qudt-schema": "http://qudt.org/schema/qudt/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",

        "Quantity": {
            "@id": "qudt-schema:Quantity",
            "@context": {
                "unit": {
                    "@id": "qudt-schema:unit",
                    "@type": "@id",
                    "@context": {
                        "@base": "http://qudt.org/vocab/unit/"
                    }
                },
                "value": {
                    "@id": "qudt-schema:value",
                    "@type": "xsd:decimal"
                },
                "dimensions": {
                    "@id": "qudt-schema:dimensions",
                    "@type": "xsd:decimal"
                }
            }
        },
        "quantity-kind": {
            "@id": "qudt-schema:hasQuantityKind",
            "@type": "@id",
            "@context": {
                "@base": "http://qudt.org/vocab/quantitykind/"
            }
        },
        "unit": {
            "@id": "qudt-schema:unit",
            "@type": "@id",
            "@context": {
                "@base": "http://qudt.org/vocab/unit/"
            }
        }
    }
}
